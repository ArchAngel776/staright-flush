{
    "compilerOptions": {
        "allowJs": true,
        "strict": true,
        "alwaysStrict": true,
        "module": "ES2020",
        "moduleResolution": "node16",
        "target": "ES2020",
        "lib": ["es2021"],
        "noImplicitAny": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "downlevelIteration": true,
        "resolveJsonModule": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "baseUrl": ".",
        "paths": {
            "@actions/*": ["src/actions/*"],
            "@commands/*": ["src/commands/*"],
            "@config/*": ["src/config/*"],
            "@core/*": ["src/core/*"],
            "@components/*": ["src/core/components/*"],
            "@data/*": ["src/core/data/*"],
            "@decorators/*": ["src/core/decorators/*"],
            "@foundations/*": ["src/core/foundations/*"],
            "@helpers/*": ["src/core/helpers/*"],
            "@hooks/*": ["src/core/hooks/*"],
            "@events/*": ["src/events/*"],
            "@exceptions/*": ["src/exceptions/*"],
            "@models/*": ["src/models/*"],
            "@requests/*": ["src/requests/*"],
            "@scenarios/*": ["src/scenarios/*"],
            "@templates/*": ["src/templates/*"],
            "@validators/*": ["src/validators/*"],
            "@env": ["src/Env"],
            "@app": ["src/App"],
            "@route": ["src/Route"],
            "@cli": ["src/Cli"]
        },
        "noEmit": true
    },
    "include": [
        "build/",
        "src/",
        "tests/",
        "@types/"
    ],
    "exclude": [
        "client",
        "node_modules"
    ]
}